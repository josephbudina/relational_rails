<form action="/ice_cream_parlor" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <h3>Add New Parlor:</h3>
  <p>Name:</p>
  <input type='text' name='ice_cream_parlor[name]'>
  <p>Employee Count:</p>
  <input type='text' name='ice_cream_parlor[employees]'>
  <p>Open?:</p>
  <input type='text' name='ice_cream_parlor[open]'>
  <input type='submit'/>
</form>