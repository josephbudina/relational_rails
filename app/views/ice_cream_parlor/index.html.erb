<h1>Parlors<h1>

<% @parlors.each do |parlor| %>
  <h3><a href="/ice_cream_parlor/<%= parlor.id %>"><%= parlor.name %></a>Created At: <%= parlor.created_at %></h3>
  <a href="/ice_cream_parlor/<%= parlor.id  %>/edit">Edit</a>
  <form action="/ice_cream_parlor/<%= parlor.id %>" method="POST">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="delete"/>
  </form>
<% end %>
<a href="/ice_cream_parlor/new">New Ice Cream Parlor</a>
