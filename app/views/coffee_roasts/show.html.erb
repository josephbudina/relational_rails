<%= link_to "Coffee Company Index", "/coffee_companies" %></br>
<%= link_to "Coffee Roast Index", "/coffee_roasts" %></br>
<h1><%= @roast.name %></h1>
<p>Origin: <%= @roast.origin%></p>
<p>Elevation: <%= @roast.elevation%></p>
<p>Fresh? <%= check_box_tag 'yes', '1', @roast.fresh, :disabled => true %></p>
<%= link_to "Update Coffee Roast", "/coffee_roasts/#{@roast.id}/edit" %>
<p>
<%= button_to 'Delete Coffee Roast', "/coffee_roasts/#{@roast.id}", method: :delete %>
</p>
